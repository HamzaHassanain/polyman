{
  "name": "My Problem Template",
  "tag": "my-problem-template",
  "version": "1.0.0",

  "description": "A template for solving createing a problem on codeforces polygon.",

  "time-limit": 1000,
  "memory-limit": 256,

  "tags": ["implementation", "math"],

  "statements": {
    "english": {
      "title": "My Problem Template"
    },

    "russian": {
      "title": "Шаблон моей проблемы"
    }
  },

  "solutions": [
    {
      "name": "acc",
      "source": "./solutions/acc.cpp",
      "type": "main-correct"
    },
    {
      "name": "acc2",
      "source": "./solutions/acc2.java",
      "type": "correct"
    },
    {
      "name": "tle",
      "source": "./solutions/tle.py",
      "type": "tle"
    }
  ],

  "generators": [
    {
      "name": "samples",
      "source": "NA",
      "tests-range": [1, 2]
    },
    {
      "name": "MainGenerator",
      "source": "./generators/gen.cpp",
      "tests-range": [3, 30]
    }
  ],

  "checker": {
    "custom": true,
    "source": "./checker/chk.cpp"
  },

  "validator": {
    "source": "./validator/val.cpp"
  }
}
